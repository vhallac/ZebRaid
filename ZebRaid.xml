  <Button name="ZebRaidDialogButtonTemplate" virtual="true" hidden="true" parent="ZebRaidDialogPanel" movable="true">
    <Size>
      <AbsDimension x="95" y="13" />
    </Size>
    <Anchors>
      <Anchor point="TOP" />
    </Anchors>
    <Layers>
      <Layer level="BACKGROUND">
        <Texture name="$parentColor">
          <Color r="0.6941177" g="0.6" b="0.4901961" a="0.6941177" />
        </Texture>
      </Layer>
      <Layer>
        <FontString name="$parentLabel" inherits="GameFontNormal" font="Fonts\FRIZQT__.TTF" justifyH="LEFT" text="99">
          <Size>
            <AbsDimension x="74" y="12" />
          </Size>
          <Anchors>
            <Anchor point="LEFT" />
          </Anchors>
          <FontHeight>
            <AbsValue val="12" />
          </FontHeight>
          <Color r="1" g="0.8196079" b="0" />
        </FontString>
        <FontString name="$parentRoll" inherits="GameFontNormal" justifyH="RIGHT" font="Fonts\FRIZQT__.TTF" text="NAME XY">
          <Size>
            <AbsDimension x="30" y="12" />
          </Size>
          <Anchors>
            <Anchor point="RIGHT" />
          </Anchors>
          <FontHeight>
            <AbsValue val="12" />
          </FontHeight>
          <Color r="1" g="0.8196079" b="0" />
        </FontString>
      </Layer>
    </Layers>
    <Scripts>
      <OnDragStart>
        ZebRaid:PlayerOnDragStart();
      </OnDragStart>
      <OnDragStop>
        ZebRaid:PlayerOnDragStop();
      </OnDragStop>
      <OnDoubleClick>
        ZebRaid:PlayerOnDoubleClick();
      </OnDoubleClick>
      <OnEnter>
        if (this.tooltipText) then
          GameTooltip:SetOwner(this, "ANCHOR_RIGHT");
          if (this.tooltipDblLine) then
            GameTooltip:AddDoubleLine(this.tooltipDblLine.left,
                                      this.tooltipDblLine.right,
                                      NORMAL_FONT_COLOR.r,
                                      NORMAL_FONT_COLOR.g, 
                                      NORMAL_FONT_COLOR.b, 
                                      HIGHLIGHT_FONT_COLOR.r, 
                                      HIGHLIGHT_FONT_COLOR.g,
                                      HIGHLIGHT_FONT_COLOR.b);
          end
          GameTooltip:AddLine(this.tooltipText, 1, 1, 1, 1);
          GameTooltip:Show();
        end
      </OnEnter>
      <OnLeave>
        GameTooltip:Hide();
      </OnLeave>
    </Scripts>
  </Button>
